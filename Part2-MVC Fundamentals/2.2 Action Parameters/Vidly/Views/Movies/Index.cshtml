@model System.Collections.Generic.List<Vidly.Models.Movie>

@{
    ViewBag.Title = "title";
    Layout = "~/Views/Shared/_Layout.cshtml"; // this is for cs template 
}

<table>
    <tr>
        <th>Movies</th>
    </tr>
    @foreach (var movie in Model)
    {
        
        <tr>
            <td>@movie.Name</td>
        </tr>
    }
    
</table>
